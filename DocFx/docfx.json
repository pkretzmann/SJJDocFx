{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/**.al"],
          "src": ".."
        }
      ],
      "dest": "reference",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "reference/**.yml",
          "reference/index.md"
        ]
      },
      {
        "files": [
          "Articles/toc.yml",
          "Articles/**.md",
          "Articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}