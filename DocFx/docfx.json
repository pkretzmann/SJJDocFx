{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/**.al"],
          "src": ".."
        }
      ],
      "dest": "reference",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "reference/**.yml",
          "reference/index.md"
        ]
      },
      {
        "files": [
          "articles/toc.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "DocFX website",
      "_appFooter": "&#0169; 2023 Sif Jakobs ApS",
      "_enableSearch": "true",
      "_disableContribution": "true",
      "_disableAffix":  "false"
    }
  }
}